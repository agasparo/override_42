#include <stdio.h>
#include <stdlib.h>

int		main() {
	int		password;

	puts("***********************************");
	puts("* 	     -Level00 -		  *");
	puts("***********************************");

	printf("Password:\n");
	scanf("%d", &password);

	if (password == 5276) {
		puts("Authenticated!");
		system("/bin/sh");
		return (0);
	}
	puts("Invalid Password!");
	return (1);
}